[ RECORD]
[PLATFORM] WINDOWS 
[UNIQUEID] _2XF0X7GGK
[CLASS] chkpreferences
[CLASSLOC] editproperty.vcx
[BASECLASS] checkbox
[OBJNAME] chkAbbreviations
[PARENT] PREFS.Frame.PageGeneral.Frame.PageHighlighting
[START PROPERTIES]
Alignment = 0
Caption = "Use abbreviations in grid columns"
Left = 40
Name = "chkAbbreviations"
TabIndex = 3
Top = 40
[END PROPERTIES]
[START METHODS]
PROCEDURE Init
DoDefault()

This.CheckBoxSetup ('Abbreviated', .T., ccLOC_Pref_Cap_UseAbbreviations, ccLOC_Pref_Tip_UseAbbreviations)

ENDPROC
PROCEDURE Valid
DoDefault()

Local lnColumn, lnWidth
With Thisform.oCallingForm
	lnWidth = 25 * .nFontSize / 8
	With .oGrid
		.SetColumnHeadings()
		For lnColumn = 1 To .ColumnCount
			If Not Inlist(lnColumn, 1, 9, 10)
				.Columns (lnColumn).Width = Iif(This.Value, lnWidth, .Columns[lnColumn].nWidth)
			Endif
		Next lnColumn
		.Refresh()
	Endwith
Endwith

ENDPROC
[END METHODS]

[ RECORD]
[PLATFORM] WINDOWS 
[UNIQUEID] _2XF0X7GGJ
[CLASS] chkpreferences
[CLASSLOC] editproperty.vcx
[BASECLASS] checkbox
[OBJNAME] chkGridAbove
[PARENT] PREFS.Frame.PageGeneral.Frame.PageHighlighting
[START PROPERTIES]
Alignment = 0
Caption = "Position grid above other controls"
Height = 16
Left = 40
Name = "chkGridAbove"
TabIndex = 2
Top = 16
Width = 179
[END PROPERTIES]
[START METHODS]
PROCEDURE Init
DoDefault()

With This
	If PEME_IsVersion9() and Thisform.oCallingForm.Docked
		.Enabled = .F.
		lcToolTipText 	= ccLOC_Pref_Tip_GridAboveDocked
	Else
		.Enabled = .T.
		lcToolTipText 	= ccLOC_Pref_Tip_GridAbove
	EndIf
	
	.CheckBoxSetup ('GridAbove', .F., ccLOC_Pref_Cap_GridAbove, lcToolTipText)

Endwith


ENDPROC
PROCEDURE Valid
DoDefault()

With Thisform.oCallingForm
