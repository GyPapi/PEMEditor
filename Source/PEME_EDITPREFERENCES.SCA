[CLASS] editpropertycommandbutton
[CLASSLOC] editproperty.vcx
[BASECLASS] commandbutton
[OBJNAME] cmdDefaultAccess
[PARENT] PREFS.Frame.PageHighlighting.Frame.pagProcessing1
[START PROPERTIES]
AutoSize = .F.
Caption = "Default _Access Method"
Left = 94
Name = "cmdDefaultAccess"
TabIndex = 11
Top = 101
Width = 132
ZOrderSet = 8
[END PROPERTIES]
[START METHODS]
PROCEDURE Click
Local lcCode, lcNewCode
lcCode = Thisform.oPrefs.cAccessCode
lcNewCode = Thisform.EditDefaultAccess(ccLOC_Pref_ID_Default_Access, lcCode)
Thisform.oPrefs.cAccessCode = lcNewCode


ENDPROC
PROCEDURE Init
This.Caption = ccLOC_Pref_Cap_Default_Access
DoDefault()

ENDPROC
PROCEDURE MouseEnter
LPARAMETERS nButton, nShift, nXCoord, nYCoord

This.ToolTipText = ccLOC_Pref_Tip_Default_Access

ENDPROC
PROCEDURE RightClick
Local loMenu As 'ContextMenu' Of  'PEME_Menu.vcx'
Local lcChoice, lcCode, lcNewCode

loMenu = Newobject ('ContextMenu', 'PEME_Menu.vcx')

With loMenu
	If .AddSubMenu('Simple (non-array) properties')
		.AddMenuItem(pf_LOC_CMA_Edit, 'Edit Scalar')
		.AddMenuItem(pf_LOC_CMA_Reset, 'Scalar Default')
		.EndSubmenu()
	Endif

	If .AddSubMenu('Array properties')
		.AddMenuItem(pf_LOC_CMA_Edit, 'Edit Array')
		.AddMenuItem(pf_LOC_CMA_Reset, 'Array Default')
		.EndSubmenu()
	Endif
Endwith

If Not loMenu.Activate()
	Return
Endif

lcChoice = loMenu.Keyword

Do Case

	Case lcChoice = 'Edit Scalar'
		lcCode = Thisform.oPrefs.cAccessCode
		lcNewCode = Thisform.EditDefaultAccess(ccLOC_Pref_ID_Default_Access, lcCode)
		Thisform.oPrefs.cAccessCode = lcNewCode

	Case lcChoice = 'Scalar Default'
		Thisform.oPrefs.cAccessCode = .Null.

	Case lcChoice = 'Edit Array'
		lcCode = Thisform.oPrefs.cArrayAccessCode
		lcNewCode = Thisform.EditDefaultAccess(ccLOC_Pref_ID_Default_Access, lcCode)
		Thisform.oPrefs.cArrayAccessCode = lcNewCode

	Case lcChoice = 'Array Default'
		Thisform.oPrefs.cArrayAccessCode = .Null.

Endcase


ENDPROC
[END METHODS]

