cfoxuserid = Preferences
cfoxusername = EditPMPreferences
changefontsize.Name = "changefontsize"
chelpfile = 
lpersistsizeposition = .F.
lreturnvalue = .F.
lsavesettings = .T.
nhelpid = 0
noriginaltooltiptimeout = 0
npage = 0
ntop = 0
oprefs = .NULL.
[END PROPERTIES]
[START METHODS]
PROCEDURE Activate
If Not Empty (This.nPage)
	This.Frame.Pages(This.nPage).SetFocus()
	This.nPage = 0
Endif


ENDPROC
PROCEDURE Destroy
_TooltipTimeout = This.nOriginalToolTipTimeout
DoDefault()

ENDPROC
PROCEDURE Init
Lparameters loPrefs, lnPage
Local lnHeight, lnLeft, lnTop, lnWidth

With This

	.oPrefs = loPrefs
	.oPrefs.CopySettings() && so we can restore them, if cancelled.

	DoDefault()

	.nOriginalToolTipTimeout = _ToolTipTimeOut
	_ToolTipTimeOut = 0
	
	.Caption = ccLOC_Pref_Cap_PREFERENCES
	.Refresh()

	.nPage = lnPage
	
Endwith

ENDPROC
PROCEDURE QueryUnload
lnResponse = MessageBox('Close the form, discarding all changes?', 32 + 3)
If lnResponse # 6
	NoDefault
	Return
EndIf
This.lReturnValue = .F.
ENDPROC
PROCEDURE Unload
Return This.lReturnValue

ENDPROC
PROCEDURE editdefaultaccess
Lparameters tcCaption, lcCode

Local lcFileName, lcWindowName 
lcWindowName = "Default.Property" + tcCaption
lcFileName = Sys(2023) + "\" + lcWindowName

*** JRN 12/18/2008 : if already being shown ... close it
If Wexist(lcWindowName)
	Release Windows (lcWindowName)
Endif
Strtofile(lcCode, lcFileName, 0)

Modify Command (lcFileName)

Return Filetostr(lcFileName)


ENDPROC
PROCEDURE setgridhighlighting
Lparameters lcNewSetting

With Thisform
	.oPrefs.cGridHighLights = lcNewSetting
	.Refresh()
EndWith 

ENDPROC
